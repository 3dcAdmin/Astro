{
  "name": "with-stencil",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/stencil/out/index.cjs.js",
  "module": "./dist/stencil/out/index.js",
  "es2015": "dist/stencil/out/esm/index.mjs",
  "es2017": "dist/stencil/out/esm/index.mjs",
  "types": "dist/stencil/out/types/components.d.ts",
  "collection": "dist/stencil/out/collection/collection-manifest.json",
  "collection:main": "dist/stencil/out/collection/index.js",
  "unpkg": "dist/stencil/out/with-stencil/with-stencil.esm.js",
