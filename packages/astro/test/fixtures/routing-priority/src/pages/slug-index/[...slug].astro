---
export function getStaticPaths() {
	return [{
		params: { slug: "index" },
	}, {
		params: { slug: 'potato' },
  }]
}
const { slug } = Astro.params
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width" />
		<title>{slug}</title>
	</head>
	<body>
		<h1>slug-index/[...slug].astro</h1>
		<p>slug: {slug || 'index'}</p>
	</body>
</html>
