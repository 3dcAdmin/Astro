---
import {getEntryBySlug} from "astro:content";

const blogEntry = await getEntryBySlug("blog", "introduction");
const {Content} = blogEntry.render()
---
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>It's content time!</title>
  </head>
  <body>
    <main>
      <Content />
    </main>
  </body>
</html>
