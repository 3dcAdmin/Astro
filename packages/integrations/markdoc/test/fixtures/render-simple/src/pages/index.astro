---
import { getEntryBySlug } from "astro:content";

const post = await getEntryBySlug('blog', 'simple');
const { Content } = await post.render();
---

<html lang="en">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width">
	<title>Content</title>
</head>
<body>
	<Content />	
</body>
</html>
