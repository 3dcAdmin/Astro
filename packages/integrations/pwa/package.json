{
  "name": "@astrojs/pwa",
  "version": "0.0.0",
  "description": "Use vite-plugin-pwa within Astro",
  "type": "module",
  "types": "./dist/index.d.ts",
  "author": "withastro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/withastro/astro.git",
    "directory": "packages/integrations/pwa"
  },
  "keywords": [
    "astro",
    "astro-integration",
    "service-worker",
    "pwa"
  ],
  "bugs": "https://github.com/withastro/astro/issues",
  "homepage": "https://astro.build",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
