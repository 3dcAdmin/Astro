{
  "name": "@astrojs/cloudflare",
  "description": "Astro SSR app for Cloudflare Pages",
  "version": "0.0.1",
  "type": "module",
  "types": "./dist/index.d.ts",
  "author": "withastro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/withastro/astro.git",
    "directory": "packages/integrations/cloudflare"
  },
  "bugs": "https://github.com/withastro/astro/issues",
  "homepage": "https://astro.build",
  "exports": {
    ".": "./dist/index.js",
    "./integration-worker": "./dist/integration-worker.js",
    "./cloudflare-worker": "./dist/cloudflare-worker.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "astro-scripts build \"src/**/*.ts\" && tsc",
    "build:ci": "astro-scripts build \"src/**/*.ts\"",
    "dev": "astro-scripts dev \"src/**/*.ts\"",
    "test-worker": "npx mocha --exit --timeout 20000 test/",
    "test": "npm run test-worker"
  },
  "dependencies": {
    "@astrojs/webapi": "workspace:*"
  },
  "devDependencies": {
    "astro": "workspace:*",
    "astro-scripts": "workspace:*"
  }
}
